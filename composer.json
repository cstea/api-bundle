{
    "name": "cstea/api-bundle",
    "description": "Symfony bundle providing an opinionated toolkit for building RESTful APIs",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "doctrine/annotations": "^1.6",
        "lcobucci/jwt": "^3.2",
        "symfony/property-info": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/security-guard": "^4.1",
        "symfony/serializer-pack": "^1.0",
        "symfony/validator": "^4.1",
        "symfony/event-dispatcher": "^4.1"
    },
    "require-dev": {
        "codeception/codeception": "^2.4",
        "mockery/mockery": "^1.1",
        "nubs/random-name-generator": "^2.1",
        "slevomat/coding-standard": "^4.8"
    },
    "autoload": {
        "psr-4": { "Cstea\\ApiBundle\\": "" }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Stea",
            "email": "cstea@icloud.com"
        }
    ],
    "minimum-stability": "stable"
}
